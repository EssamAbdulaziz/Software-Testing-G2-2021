<?xml version="1.0" encoding="UTF-8"?>
<extentreports>
    <configuration>
        <!-- report theme -->

        <!-- standard, dark -->

        <theme>standard</theme>
        <!-- document encoding -->

        <!-- defaults to UTF-8 -->

        <encoding>UTF-8</encoding>
        <!-- protocol for script and stylesheets -->

        <!-- defaults to https -->

        <protocol>http</protocol>
        <!-- title of the document -->
        <documentTitle>Extent Report</documentTitle>
        <!-- report name - displayed at top-nav -->

        <reportName>Automation Practice</reportName>
        <!-- location of charts in the test view -->

        <!-- top, bottom -->

        <testViewChartLocation>bottom</testViewChartLocation>
        <!-- custom javascript -->

        <!-- settings to enable/disable views -->
        <!-- professional version only -->
        <enableCategoryView>true</enableCategoryView>
        <enableAuthorView>true</enableAuthorView>
        <enableExceptionView>true</enableExceptionView>
        <enableTestRunnerLogsView>true</enableTestRunnerLogsView>

        <scripts>

            <![CDATA[
$(document).ready(function() {
});
]]>
        </scripts>
        <!-- custom styles -->
        <styles>
            <![CDATA[
]]>
        </styles>
    </configuration>
</extentreports>